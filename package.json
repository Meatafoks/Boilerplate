{
  "name": "my-app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "jest"
  },
  "main": "./build/index.js",
  "prettier": "@metafoks/prettier",
  "eslintConfig": {
    "extends": [
      "@metafoks/eslint-config"
    ],
    "env": {
      "jest": true
    },
    "plugins": [
      "jest"
    ]
  },
  "dependencies": {
    "metafoks-application": "1.0.12",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@metafoks/eslint-config": "1.1.0",
    "@metafoks/prettier": "1.1.0",
    "@types/jest": "29.4.0",
    "eslint": "8.56.0",
    "eslint-plugin-jest": "27.9.0",
    "jest": "29.4.3",
    "prettier": "3.2.5",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.2"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "<rootDir>/src/index.ts",
      ".mock.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "testRegex": "(/test/.*\\.(test|spec))\\.(jsx?|tsx?|ts|js)$",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node",
      "jsx",
      "tsx"
    ]
  }
}
